{
  "extends": "./../../tsconfig.build.json",
  "compilerOptions": {
    "outDir": "./",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "resolveJsonModule": false,
    "rootDir": "./src",
    "allowJs": true,
    "checkJs": false,
    "emitDeclarationOnly": true
  },
  "include": ["./src/server/*", "./src/*.d.ts", "./src/types.ts"]
}
