{
  "name": "@snowstorm/core",
  "version": "0.0.1-alpha.3",
  "license": "MIT",
  "author": "<mail@henrygressmann.de>",
  "exports": {
    "./server": "./server.js",
    "./server/export": "./server/export.js",
    "./": "./index.js"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf -rf index.d.ts index.js index.js.map server.d.ts server.js server.js.map types.d.ts types.js types.js.map server/ client/",
    "compile": "run-s compile:server compile:client",
    "compile:client": "tsc -p tsconfig.client.json",
    "compile:server": "tsc -p tsconfig.server.json",
    "watch": "run-p watch:*",
    "watch:client": "tsc -p tsconfig.client.json --watch",
    "watch:server": "tsc -p tsconfig.server.json --watch"
  },
  "dependencies": {
    "@snowpack/plugin-postcss": "^1.2.2",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowstorm/head": "0.0.1-alpha.3",
    "@snowstorm/serverprops": "0.0.1-alpha.3",
    "chokidar": "^3.5.1",
    "deepmerge": "^4.2.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "glob-promise": "^4.1.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-html-minifier": "^2.1.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.6.1",
    "require-from-string": "^2.0.2",
    "snowpack": "^3.3.7",
    "typescript": "^4.2.4",
    "website-scraper": "^4.2.3",
    "wouter": "^2.7.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/koa": "^2.13.1",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-html-minifier": "^1.0.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/require-from-string": "^1.2.0",
    "@types/sass": "^1.16.0",
    "@types/snowpack-env": "^2.3.3",
    "@types/website-scraper": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "type-fest": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
