{
  "name": "@snowstorm/core",
  "version": "0.2.0",
  "license": "MIT",
  "author": "<mail@henrygressmann.de>",
  "type": "module",
  "exports": {
    "./server": "./server.js",
    "./server/export": "./server/export.js",
    ".": "./index.js"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "server.js",
    "server.d.ts",
    "client/",
    "server/",
    "assets/",
    "chunks/"
  ],
  "scripts": {
    "build": "run-s clean build-all",
    "build-all": "run-p --print-label build:**",
    "build:client": "esbuild src/client/index.tsx src/client/load-html.tsx src/client/_error.tsx src/client/router/index.tsx --bundle --splitting --chunk-names=../chunks/[name]-[hash] --outdir=client --platform=node --target=node16 --format=esm --metafile=client.meta.json --external:_snowstorm* --external:@snowstorm/* --external:react --external:react-dom --minify",
    "build:index": "esbuild src/index.ts --outdir=. --platform=node --target=node16 --format=esm --minify",
    "build:server": "esbuild src/server.ts src/server/export.ts --bundle --splitting --chunk-names=chunks/[name]-[hash] --outdir=. --platform=node --target=node16 --format=esm --metafile=server.meta.json --external:typescript --external:node-fetch --external:vite --external:@vitejs/* --banner:js='import { createRequire as topLevelCreateRequire } from \"module\";\n const require = topLevelCreateRequire(import.meta.url);'",
    "build:types": "run-p --print-label types:**",
    "clean": "rimraf -rf index.d.ts index.js index.js.map server.d.ts server.js server.js.map types.d.ts types.js types.js.map server/ client/",
    "types:client": "tsc -p tsconfig.client.json",
    "types:server": "tsc -p tsconfig.server.json",
    "watch": "run-p --print-label \"build:** -- --watch\""
  },
  "dependencies": {
    "@snowstorm/head": "link:../head",
    "@snowstorm/serverprops": "link:../serverprops",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "tslog": "^3.2.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.2",
    "wouter": "^2.7.5"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^16.10.2",
    "@types/react": "17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/website-scraper": "^1.2.6",
    "chokidar": "^3.5.2",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.13.3",
    "esbuild-visualizer": "^0.3.1",
    "fast-glob": "^3.2.7",
    "koa": "^2.13.3",
    "koa-compress": "^5.1.0",
    "koa-connect": "^2.1.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "node-fetch": "^3.0.0",
    "react": "18.0.0-alpha-f2c381131-20211004",
    "react-dom": "18.0.0-alpha-f2c381131-20211004",
    "type-fest": "^2.3.4",
    "website-scraper": "^4.2.3"
  },
  "peerDependencies": {
    "react": "18.0.0-alpha-f2c381131-20211004",
    "react-dom": "18.0.0-alpha-f2c381131-20211004"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
