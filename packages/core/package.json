  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./server": "./server.js",
    "./": "./index.js"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf -rf index.d.ts index.js index.js.map server.d.ts server.js server.js.map  server/ client/",
    "compile": "run-s compile:server compile:client",
    "compile:client": "tsc -p tsconfig.client.json",
    "compile:server": "tsc -p tsconfig.server.json",
    "watch": "run-p watch:*",
    "watch:client": "tsc -p tsconfig.client.json --watch",
    "watch:server": "tsc -p tsconfig.server.json --watch"
  },
  "dependencies": {
    "@snowpack/plugin-react-refresh": "^2.4.1",
    "@snowstorm/serverprops": "0.0.0-beta.1",
    "@snowstorm/head": "0.0.0-beta.1",
    "@types/koa-compress": "^4.0.1",
    "chokidar": "^3.5.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "glob-promise": "^4.1.0",
    "koa": "^2.13.1",
    "koa-cash": "^4.0.5",
    "koa-compress": "^5.0.1",
    "koa-html-minifier": "^2.0.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.6.1",
    "react-helmet": "^6.1.0",
    "require-from-string": "^2.0.2",
    "snowpack": "^3.1.2",
    "typescript": "^4.0.0",
    "wouter": "^2.7.4",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "@types/koa-cash": "^4.0.0",
    "@types/koa-html-minifier": "^1.0.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/require-from-string": "^1.2.0",
    "@types/snowpack-env": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependenciesMeta": {
