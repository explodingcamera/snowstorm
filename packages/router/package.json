{
  "name": "@snowstorm/router",
  "version": "0.9.0",
  "license": "MIT",
  "author": "<mail@henrygressmann.de>",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./lib": "./lib",
    "./lib/matcher": "./lib/matcher.js",
    "./lib/static-location": "./lib/static-location.js"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "run-p --print-label build:esbuild types",
    "build:esbuild": "esbuild src/*.ts src/server/index.ts --minify --bundle --splitting --chunk-names=chunks/[name]-[hash] --outdir=lib --platform=node --external:react --target=node16 --format=esm",
    "watch": "npm run build -- --watch",
    "types": "tsc -p tsconfig.build.json",
    "test": "tap --ts --node-arg=--loader=ts-node/esm"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "17.0.38",
    "@types/tap": "^15.0.5",
    "esbuild": "^0.14.2",
    "global-jsdom": "^8.4.0",
    "jsdom": "^19.0.0",
    "react": "^18.0.0-rc.0",
    "tap": "^15.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
