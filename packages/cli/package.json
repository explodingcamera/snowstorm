{
  "name": "@snowstorm/cli",
  "version": "0.0.0-beta.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "snowstorm": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "rm -rf lib",
    "prepack": "yarn run clean && yarn run build && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@snowstorm/core": "0.0.0-beta.1",
    "ts-node": "^8",
    "tslib": "^1",
    "typescript": "^3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "snowstorm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
