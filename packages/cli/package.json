{
  "name": "@snowstorm/cli",
  "version": "0.0.1-alpha.3",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "snowstorm": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "run-s clean compile && oclif-dev manifest && oclif-dev readme && yarn postpack",
    "compile": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf lib",
    "postpack": "rimraf oclif.manifest.json"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^3.2.3",
    "@snowstorm/core": "0.0.1-alpha.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^16.7.4",
    "globby": "^12.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "snowstorm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
